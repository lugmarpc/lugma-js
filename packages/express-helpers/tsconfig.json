{
	"compilerOptions": {
		"target": "ES6",
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["es2015", "WebWorker"],
		"downlevelIteration": true,
		"declaration": true,
		"declarationDir": "dist"
	},
	"include": ["src/**/*"]
}
